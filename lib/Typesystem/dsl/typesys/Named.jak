layer typesys;

/**
 * Implementierungen von <code>Named</code> müssen einen Namen haben.
 * 
 * @layer typesys
 * @author Stephan Klapproth
 *
 */
public interface Named {

	/**
	 * Gibt den vollständig qualifizierten Namen des Objekts zurück.
	 * 
	 * @return vollständig qualifizierter Name des Objekts.
	 */
	String getFullName();

	/**
	 * Gibt den Namen des Objekts zurück.
	 * 
	 * @return Name des Objekts.
	 */
	String getName();
}
