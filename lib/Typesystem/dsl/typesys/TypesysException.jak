layer typesys;

/**
 * Allgemeine ungeprüfte Exception, verursacht innerhalb des Typsystems.
 * 
 * @layer typesys
 * @author Stephan Klapproth
 * 
 */
public class TypesysException extends RuntimeException {

    private static final long serialVersionUID = -8608468864917079442L;
	
	public TypesysException(String aMessage) {
		super(aMessage);
	}
	
	public TypesysException(Throwable e) {
		super(e);
	}
	
}
